<?xml version="1.0" encoding="utf-8" ?>

<!--
Arctic
Desert
Jungle
Stone
Urban
Woodland
-->

<Patch>

	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/BiomeDef[defName="BMT_Atoll" or @Name="BMT_TropicalIslandBase"]</xpath>
		<value>
			<li Class="CompCamo.CompCamoDefs">
				<CamoType>Jungle</CamoType>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/BiomeDef[@Name="BMT_DesertIslandBase"]</xpath>
		<value>
			<li Class="CompCamo.CompCamoDefs">
				<CamoType>Desert</CamoType>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/BiomeDef[@Name="BMT_BorealIslandBase" or @Name="BMT_TemperateIslandBase"]</xpath>
		<value>
			<li Class="CompCamo.CompCamoDefs">
				<CamoType>Woodland</CamoType>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/BiomeDef[@Name="BMT_TundraIslandBase"]</xpath>
		<value>
			<li Class="CompCamo.CompCamoDefs">
				<CamoType>Arctic</CamoType>
			</li>
		</value>
	</Operation>

</Patch>

