	<ThingDef ParentName="EggFertBase">
		<defName>BMT_EggSeagullFertilized</defName>
		<label>seagull egg (fert.)</label>
		<description>A fertilized seagull egg. If all goes well, it should hatch into a baby seagull. It can be eaten raw, but it's much better cooked.</description>
		<graphicData>
			<color>(236,217,184)</color>
			<drawSize>1.1</drawSize>
		</graphicData>
		<statBases>
			<MarketValue>28.0</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Hatcher">
				<hatcherDaystoHatch>5</hatcherDaystoHatch>
				<hatcherPawn>BMT_Seagull</hatcherPawn>
			</li>
		</comps>
	</ThingDef>

	
	<ThingDef ParentName="EggUnfertBase">
		<defName>BMT_EggSeagullUnfertilized</defName>
		<label>seagull egg (unfert.)</label>
		<description>An unfertilized seagull egg. It can be eaten raw, but it's much, much better cooked.</description>
		<graphicData>
			<color>(226,207,174)</color>
			<drawSize>1.1</drawSize>
		</graphicData>
		<statBases>
			<MarketValue>13.0</MarketValue>
		</statBases>
	</ThingDef>
	
	
</Defs>