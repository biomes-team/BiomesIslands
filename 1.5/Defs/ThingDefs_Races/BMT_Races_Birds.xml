	<ThingDef ParentName="BMT_BirdThingBase">
		<defName>BiomesIslands_Seagull</defName>
		<label>seagull</label>
		<description>Noisy birds that eat anything small enough to fit down their gullets. They live in large colonies, and will happily prey on and swallow whole any animal they can.</description>
		<statBases>
			<MoveSpeed>3</MoveSpeed>
			<MarketValue>95</MarketValue>
			<ComfyTemperatureMin>-25</ComfyTemperatureMin>
			<ComfyTemperatureMax>50</ComfyTemperatureMax>
		</statBases>
		<race>
			<herdAnimal>true</herdAnimal>
			<predator>true</predator>
			<maxPreyBodySize>0.20</maxPreyBodySize>
			<baseBodySize>0.45</baseBodySize>
			<baseHealthScale>0.5</baseHealthScale>
			<baseHungerRate>0.5</baseHungerRate>
			<foodType>OmnivoreRoughAnimal</foodType>
			<wildness>0.45</wildness>
			<trainability>Intermediate</trainability>
			<lifeExpectancy>11</lifeExpectancy>
			<manhunterOnTameFailChance>0.02</manhunterOnTameFailChance>
			<manhunterOnDamageChance>0.013</manhunterOnDamageChance>
			<lifeStageAges>
				<li>
					<def>BiomesIslands_SeagullBabyTiny</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>BiomesIslands_SeagullJuvenile</def>
					<minAge>0.22</minAge>
				</li>
				<li>
					<def>BiomesIslands_SeagullAdult</def>
					<minAge>0.45</minAge>
				</li>
			</lifeStageAges>
		</race>
		<modExtensions>
			<li Class="PathfindingFramework.MovementExtension">
				<movementDef>PF_Movement_Flying</movementDef>
			</li>
			<li Class="PathfindingFramework.LocomotionMovementExtension">
				<locomotionUrgencies>
					<li>Jog</li>
					<li>Sprint</li>
				</locomotionUrgencies>
				<moveSpeedMultiplier>4</moveSpeedMultiplier>
			</li>
		</modExtensions>
		<comps>
			<li Class="CompProperties_EggLayer">
				<eggFertilizedDef>BiomesIslands_EggSeagullFertilized</eggFertilizedDef>
				<eggFertilizationCountMax>2</eggFertilizationCountMax>
				<eggLayIntervalDays>9</eggLayIntervalDays>
				<eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
				<eggCountRange>
					<min>2</min>
					<max>4</max>
				</eggCountRange>
			</li>
		</comps>
		<tools>
			<li>
				<label>claws</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>4.5</power>
				<cooldownTime>1.4</cooldownTime>
				<linkedBodyPartsGroup>Feet</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>beak</label>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>4</power>
				<cooldownTime>1.8</cooldownTime>
				<linkedBodyPartsGroup>Beak</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>2</power>
				<cooldownTime>1.8</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<tradeTags>
			<li>AnimalUncommon</li>
			<li>AnimalPet</li>
		</tradeTags>
	</ThingDef>

