				<eggFertilizedDef>BiomesIslands_EggOmegaDodoFertilized</eggFertilizedDef>
				<eggUnfertilizedDef>BiomesIslands_EggOmegaDodoUnfertilized</eggUnfertilizedDef>
				<eggFertilizationCountMax>1</eggFertilizationCountMax>
				<eggLayIntervalDays>4</eggLayIntervalDays>
				<eggProgressUnfertilizedMax>1</eggProgressUnfertilizedMax>
				<eggCountRange>
					<min>1</min>
					<max>3</max>

				</eggCountRange>
			</li>
		</comps>
		<tools>
			<li>
				<label>claws</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>6</power>
				<cooldownTime>1.4</cooldownTime>
				<linkedBodyPartsGroup>Feet</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>20</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>beak</label>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>18</power>
				<cooldownTime>1.8</cooldownTime>
				<linkedBodyPartsGroup>Beak</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>10</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
